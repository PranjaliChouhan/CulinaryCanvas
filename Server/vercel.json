{
    "version": 2,
    "builds": [
      {
        "src": "myapp/package.json",
        "use": "@vercel/static-build",
        "config": { "buildCommand": "npm install && npm run build" }
      },
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "server.js" },
      { "src": "/(.*)",
      "dest": "myapp/src/index.js" }
    ]
  }
  